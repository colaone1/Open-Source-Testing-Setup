{
  "name": "weather-app-fullstack",
  "version": "1.0.0",
  "description": "Weather App with focus on testing and performance",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run test:backend && npm run test:frontend && npm run test:performance && npm run test:e2e",
    "test:backend": "jest tests/backend --coverage",
    "test:frontend": "jest tests/frontend --coverage",
    "test:performance": "jest tests/performance",
    "test:e2e": "cypress run",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest -u",
    "cypress:open": "cypress open",
    "docs": "jsdoc -c jsdoc.json",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint .",
    "build": "webpack --mode production",
    "analyze": "webpack-bundle-analyzer",
    "test:server": "NODE_ENV=test node server.js",
    "test:all": "concurrently \"npm run test:server\" \"npm run test\" \"npm run test:e2e\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "kcors": "^2.2.2",
    "koa": "^2.14.2",
    "koa-router": "^12.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.15.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "css-loader": "^6.8.1",
    "cypress": "^12.17.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jsdoc": "^3.6.7",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^2.0.12",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.4",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "",
  "license": "MIT"
}
